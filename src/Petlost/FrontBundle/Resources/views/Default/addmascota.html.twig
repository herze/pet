{{ form_errors(formm) }}

<div>
{{ form_row(formm.nombre) }}
</div>

<div>
{{ form_label(formm.categoria) }}
{{ form_errors(formm.categoria) }}
{{ form_widget(formm.categoria, { 'attr': { 'class': 'largo' } }) }} 
