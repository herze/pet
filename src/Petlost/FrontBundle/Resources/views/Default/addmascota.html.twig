<div>
{{ form_label(formm.sexo) }}
{{ form_errors(formm.sexo) }}
{{ form_widget(formm.sexo, { 'attr': { 'class': 'largo' } }) }}
</div>

        <div>
        {{ form_label(formm.raza) }}
